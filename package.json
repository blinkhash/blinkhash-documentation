{
  "name": "blinkhash-documentation",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-2.0",
  "author": {
    "name": "Nick Sarris"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "eslint . --ext .js --ignore-pattern node_modules/",
    "clean:fix": "eslint . --ext .js --fix --ignore-pattern node_modules/",
    "dev:webpack": "NODE_ENV=development webpack --config webpack.dev.js --mode=development -w",
    "dev:build": "npm-run-all --parallel build dev:webpack",
    "dev:start": "nodemon ./dev-build/index.js",
    "prod:webpack": "NODE_ENV=production webpack --config webpack.prod.js --mode=production -w",
    "prod:build": "npm-run-all --parallel build prod:webpack",
    "prod:start": "pm2 start ./prod-build/index.js --max-restarts=5",
    "prod:stop": "pm2 stop ./prod-build/index.js"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkhash/blinkhash-documentation.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "bootstrap": "4.5.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "4.2.0",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "isomorphic-style-loader": "5.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "pm2": "4.4.0",
    "react-router-config": "^5.1.1",
    "speed-measure-webpack-plugin": "1.3.3",
    "webpack-cli": "3.3.12",
    "webpack-merge": "5.1.1",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12.16"
  },
  "readmeFilename": "README.md"
}
